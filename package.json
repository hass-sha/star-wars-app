{
  "name": "star-wars-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-local": "ng serve",
    "start": "node server/.",
    "local-app-init": "npm run postinstall && npm start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server-prebuild": "tslint -c server/tslint.json -p server/tsconfig.json --fix",
    "server-build": "tsc --p server",
    "client-build": "ng build --prod --progress=true",
    "postinstall": "npm run client-build && npm run server-prebuild && npm run server-build",
    "postinstall-local": "npm run server-prebuild && npm run server-build",
    "nodemon-local": "nodemon --watch src --watch server/src --ext * --exec npm run local-app-init -V"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.7.37",
    "@types/request": "^2.48.4",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "ngx-spinner": "^8.1.0",
    "nodemon": "^2.0.2",
    "popper.js": "^1.16.1",
    "request": "^2.88.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
